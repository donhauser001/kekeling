    "@tarojs/runtime": "4.1.9",
    "@tarojs/shared": "4.1.9",
    "@tarojs/taro": "4.1.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@tarojs/cli": "4.1.9",
    "@tarojs/webpack5-runner": "4.1.9",
    "@types/react": "^18.3.1",
    "eslint": "^9.0.0",
    "sass": "^1.80.0",
    "typescript": "^5.6.3"
  }
